<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StoreClerk.StatRetActivity">

    <fragment
        android:id="@+id/retFragment1"
        android:name="com.logicuniv.mlussis.StoreClerk.StatRetMenuFragment"
        android:layout_width="match_parent"
        android:layout_height="70dp"></fragment>

    <FrameLayout
        android:id="@+id/retDetailsFragment"
        android:name="com.logicuniv.mlussis.StoreClerk.StatRetTableFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/retFragment1"></FrameLayout>

</RelativeLayout>